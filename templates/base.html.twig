<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        
        {# BOOTSRAP #}
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
        <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    
        {# LIEN CAROUSEL #}
        <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@ 1.8.1/slick/slick.css"/>
        <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css"/>

        {# LIEN CSS #}
        <link rel="stylesheet" href="{{asset('css/navbar.css')}}">
        <link rel="stylesheet" href="{{asset('css/paramDeBase.css')}}">
        <link rel="stylesheet" href="{{asset('css/home.css')}}">
        <link rel="stylesheet" href="{{asset('css/medias.css')}}">
        <link rel="stylesheet" href="{{asset('css/croixQui.css')}}">
        <link rel="stylesheet" href="{{asset('css/croixAvis.css')}}">
        <link rel="stylesheet" href="{{asset('css/meubles.css')}}">

    </head>
    <body>

        <div class="fakeNavbar"></div>

        <nav class="navbar fixed-top navbar-expand-lg navbar-light">

            <a class="navbar-brand" href="{{path('home')}}"><img src="{{asset('imgs/logos/logoNav.svg')}}" alt="logo" class="logoNavBar my-3"></a>
            
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse text-center" id="navbarSupportedContent">

                
                <ul class="navbar-nav m-auto">
                    <li class="nav-item">
                        <a class="nav-link {% if nav_activ is defined and nav_activ == 'home' %}navActiv{% endif %}" href="{{path('home')}}">Accueil</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle {% if nav_activ is defined and nav_activ == 'meubles' %}navActiv{% endif %}" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Meubles
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="{{path('meubles')}}#table">Tables à manger</a>
                            <a class="dropdown-item" href="{{path('meubles')}}#test">MEUBLE 2</a>
                            <a class="dropdown-item" href="#">MEUBLE 3</a>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Avis clients</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Devis</a>
                    </li>
                </ul>

                <div class="logoRS">
                    <a href="#"><img src="{{asset('imgs/r-s/facebook.svg')}}" alt="Facebook"></a>
                    <a href="#"><img src="{{asset('imgs/r-s/instagram.svg')}}" alt="Instagram"></a>
                    <a href="#"><img src="{{asset('imgs/r-s/etsy.svg')}}" alt="Etsy"></a>
                </div>

            </div>

        </nav>

        {% block body %}{% endblock %}
        <div class="backFonduFooter"></div>
        <footer>
            <div class="footerOrga">
                <div class="croquisFooter">
                    <img src="{{asset('imgs/dessins/dessin4.jpg')}}" alt="Croquis" class="img-fluid">
                </div>
                <div class="text-center">
                    <img src="{{asset('imgs/logos/logo.svg')}}" alt="Logo MBN" class="logoFooter">
                    <p>
                        Copyright ©</br>
                        Tous droits réservés à</br>
                        meublesbynicolas.com
                    </p>
                </div>
            </div>
        </footer>

        {% block javascripts %}{% endblock %}
        {# BOOTSRAP #}
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
        
        {# JAVASCRIPT #}
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
        <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
        <script src="js/croixQui.js"></script>
        <script src="js/essences.js"></script>

    </body>
</html>
